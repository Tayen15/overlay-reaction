{
     "functions": {
          "src/app/api/reactions/route.ts": {
               "maxDuration": 30
          }
     },
     "rewrites": [
          {
               "source": "/api/reactions",
               "destination": "/api/reactions"
          }
     ],
     "headers": [
          {
               "source": "/api/reactions",
               "headers": [
                    {
                         "key": "Cache-Control",
                         "value": "no-cache"
                    },
                    {
                         "key": "Connection",
                         "value": "keep-alive"
                    },
                    {
                         "key": "Content-Type",
                         "value": "text/plain; charset=utf-8"
                    }
               ]
          }
     ]
}